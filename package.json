{
  "name": "journee",
  "version": "0.1.0",
  "private": true,
  "description": "AIとともに旅のしおりを作成するアプリケーション",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e:seed": "npx tsx scripts/seed-e2e-data.ts",
    "e2e:test": "playwright test",
    "e2e:show": "playwright show-report",
    "e2e:test:trace": "playwright test --trace on",
    "e2e:test:all": "npm run test && npm run test:e2e",
    "docker:start": "bash scripts/docker-dev.sh start",
    "docker:stop": "bash scripts/docker-dev.sh stop",
    "docker:restart": "bash scripts/docker-dev.sh restart",
    "docker:logs": "bash scripts/docker-dev.sh logs",
    "docker:shell": "bash scripts/docker-dev.sh shell",
    "docker:build": "bash scripts/docker-dev.sh build",
    "docker:clean": "bash scripts/docker-dev.sh clean",
    "docker:status": "bash scripts/docker-dev.sh status",
    "deploy:gcr": "bash scripts/deploy-gcr.sh",
    "release": "bash scripts/tag-version.sh",
    "seed:mock-users": "npx tsx scripts/seed-mock-users.ts",
    "k8s:clean": "bash scripts/clean-manifests.sh"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@google/generative-ai": "^0.24.1",
    "@hello-pangea/dnd": "^18.0.1",
    "@supabase/supabase-js": "^2.95.3",
    "html2canvas": "^1.4.1",
    "idb": "^8.0.3",
    "jspdf": "^4.1.0",
    "lucide-react": "^0.564.0",
    "nanoid": "^5.1.6",
    "next": "^16.1.6",
    "next-auth": "^4.24.13",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "recharts": "^3.7.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/google.maps": "^3.58.1",
    "@types/html2canvas": "^1.0.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/recharts": "^2.0.1",
    "autoprefixer": "^10.4.24",
    "eslint": "^10.0.0",
    "eslint-config-next": "^16.1.6",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
